<%= form_for :player, url: password_change_path(@player) do |f| %>
  <%= f.label :password %>:
  <%= f.password_field :password %><br>

  <%= f.label :password_confirmation %>:
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Change Password" %>
<% end %><br><br>

<%= flash[:notice] %>
